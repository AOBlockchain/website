	<script type="text/javascript">
		(function ($) {
			$.toggleShowPassword = function (options) {
				var settings = $.extend({
					field: "#password",
					control: "#toggle_show_password",
				}, options);

				var control = $(settings.control);
				var field = $(settings.field)

				control.bind('click', function () {
					if (control.is(':checked')) {
						field.attr('type', 'text');
					} else {
						field.attr('type', 'password');
					}
				})
			};
		}(jQuery));
		//sticky header on scroll
		$(window).load(function () {
			$(".sticky").sticky({
				topSpacing: 0
			});
			$('#investmentTable').DataTable({
				"scrollY": "500px",
				"scrollCollapse": true,
				"paging": false,
				"select": true,
				"responsive": true
			});

			$("#genQRCode").click(function () {
				$("#genQRCode").prop("disabled", true);
				$.get("/api/configQR", function (data, status) {
					var otpCode = new QRCode(document.getElementById("otpQRPNG"), data.uri);
					var secretArr = data.otpSecret.match(/.{1,10}/g);
					$('.qrConfig').removeClass('hidden');
					for (var i = 0; i < secretArr.length; i++) {
						$('#otpKeyString').append(secretArr[i] + "<br />")
					}

					$('#otpQR').on('hidden.bs.modal', function (e) {
						$('#otpQRConfirm').modal('show');
						$('#otpConfirmBtn').on('click', function () {
							var d = 'token=' + $('#verifyToken').val();
							$.get('/api/checkOTP?' + d, function (data, status) {
								console.log(data);
								if (data.success === true) {
									$('#otrConfirmBody').html('<h3 class=\'has-error\'>2 Factor Authentication ' +
										'has been configured.</h3>');
									$('#qrOTP').remove();
									otpCode.clear();
								} else {
									$('#otrConfirmBody').html('<h3 class=\'has-error\'>Your Token was not configured ' +
										'correctly. Please try again later.</h3>');
									otpCode.clear();
								}
							});
						});
					});
				});
			});
			$.toggleShowPassword({
				field: '#otpSecretKey',
				control: '#showOTPKey'
			});
			$(function () {
				$('[data-toggle="popover"]').popover();
			})

		});

	</script>
